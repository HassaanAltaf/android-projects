<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="io.github.ledinhtri97.senddatabetweentwoactivities.MainActivity">

    <Button
        android:id="@+id/btnMoVaGui_Inten"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="handleOpenAndSent_Inten"
        android:text="Open And Sent Data (Inten)"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btnMoVaGui_Bundle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="handleOpenAndSent_Bundle"
        android:text="Open and sent data (Bundle)"
        tools:ignore="HardcodedText" />

</LinearLayout>
